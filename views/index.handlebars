<div class="content">

    <div class="post no-padding">
        <img src="lighthouse.jpg" alt="An image of a lighthouse during Sunset">
    </div>

    {{> resume}}


    <div id="links" class="post">
        <div class="post-content">
            <h2>Links:</h2>

            <div class="post-element">
                <!--- LinkedIn Embedded Profile --->
                <script src="https://platform.linkedin.com/badges/js/profile.js" async defer type="text/javascript"></script>
                <div class="badge-base LI-profile-badge" data-locale="en_US" data-size="large" data-theme="dark" data-type="VERTICAL" data-vanity="peter-lamontagne" data-version="v1"><a class="badge-base__link LI-simple-link" href="https://www.linkedin.com/in/peter-lamontagne?trk=profile-badge">Peter LaMontagne</a></div>
            </div>
            <div class="post-element">
                
                <div class="github-card" data-user="peetypeet5000" data-theme="default" data-width="300a"></div>
                <script src="https://lab.lepture.com/github-cards/widget.js"></script>
            </div>

        
        </div>
    </div>

    <section id="projects">
        {{#each projects}}
            {{> projectCard}}
        {{/each}}
    </section>
    
</div>